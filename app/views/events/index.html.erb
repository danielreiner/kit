<h1>Listing events</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Subline</th>
    <th>Artists</th>
    <th>Description</th>
    <th>Url</th>
    <th>Email</th>
    <th>Co</th>
    <th>Street</th>
    <th>Zip code</th>
    <th>City</th>
    <th>Country</th>
    <th>Start date</th>
    <th>End date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><%= event.name %></td>
    <td><%= event.subline %></td>
    <td><%= event.artists %></td>
    <td><%= event.description %></td>
    <td><%= event.url %></td>
    <td><%= event.email %></td>
    <td><%= event.co %></td>
    <td><%= event.street %></td>
    <td><%= event.zip_code %></td>
    <td><%= event.city %></td>
    <td><%= event.country %></td>
    <td><%= event.start_date %></td>
    <td><%= event.end_date %></td>
    <td><%= link_to 'Show', event %></td>
    <td><%= link_to 'Edit', edit_event_path(event) %></td>
    <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Event', new_event_path %>
